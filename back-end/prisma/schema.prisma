generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model experience {
  experienceid Int     @id @default(autoincrement())
  title        String? @db.VarChar(50)
  icon         String? @db.VarChar(30)
  description  String? @db.VarChar(180)
  link         String? @db.VarChar(150)
}

model proyectimages {
  proyectid Int?
  link      String    @id @db.VarChar(300)
  proyects  proyects? @relation(fields: [proyectid], references: [proyectid], onDelete: NoAction, onUpdate: NoAction)
}

model proyects {
  proyectid     Int             @id(map: "poyects_pkey") @default(autoincrement())
  title         String?         @db.VarChar(50)
  icon          String?         @db.VarChar(30)
  description   String?         @db.VarChar(180)
  link          String?         @db.VarChar(150)
  proyectimages proyectimages[]
}

model skills {
  skillid     Int     @id @default(autoincrement())
  title       String? @db.VarChar(50)
  icon        String? @db.VarChar(30)
  certificate String? @db.VarChar(300)
}
